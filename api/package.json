{
  "name": "todo-api",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "serve": "nodemon --inspect server.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:drop": "sequelize-cli db:drop",
    "start-db": "pg_ctl -D ./db/data -l ./db/data/logfile start",
    "stop-db": "pg_ctl -D /usr/local/var/postgres stop"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "2.20.x",
    "eslint-plugin-node": "11.0.x",
    "eslint-plugin-promise": "4.2.x",
    "eslint-plugin-standard": "4.0.x",
    "eslint-plugin-vue": "^8.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "grant-express": "^4.6.4",
    "nodemon": "^2.0.6",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "request": "^2.65.0",
    "sequelize": "^6.12.0-alpha.1"
  },
  "engines": {
    "node": "14.17.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "devDependencies": {
    "sequelize-cli": "^6.3.0"
  }
}
